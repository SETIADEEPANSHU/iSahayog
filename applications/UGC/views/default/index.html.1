{{#left_sidebar_enabled,right_sidebar_enabled=True,('message' in globals())}}
{{extend 'layout.html'}}
{{#extend 'try.html'}}

{{block header}}
<div class="main" style="position:relative;top:-300px">
<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
      <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
      <div class="item active">
        <img src="{{=URL(r=request,c='static',f='images/b5.jpg')}}" alt="Chania" width="460" height="345">
      </div>

      <div class="item">
        <img src="{{=URL(r=request,c='static',f='images/b1.jpg')}}" alt="Chania" width="460" height="345">
      </div>
    
      <div class="item">
        <img src="{{=URL(r=request,c='static',f='images/b2.jpg')}}" alt="Flower" width="460" height="345">
      </div>

      <div class="item">
        <img src="{{=URL(r=request,c='static',f='images/b3.jpg')}}" alt="Flower" width="460" height="345">
      </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev" style="color:#18aed3">
      <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next" style="color:#18aed3">
      <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>
<h3>
    <center>Select Options To Browse Problems</center>
</h3>
<hr/>
 <div class="container">
    
    <div class="row">

<!-- How To Add Font Awesome Social Icons In Blog Or Website3 -->
      <form role="form" action="{{=URL(r=request,f='Browse_problem')}}" method="post">
           <div class="col-lg-12">
              <div class="row">
	  <!--<div class="form-group col-lg-4">
  <label for="sel1">Select Country:</label>
  <select class="form-control countries" name="country" id="countryId sel1">
    <option value="">Select Country</option>
  </select>
</div>-->
                <div class="form-group col-lg-6">
                  <label for="sel12">Select State:</label>
                  <select class="form-control states" name="state" id="sel12 stateId">
                       <option value="">Select State</option>
                  </select>
                </div>

                <div class="form-group col-lg-6">
                  <label for="sel13">Select City:</label>
                  <select class="form-control cities" id="sel13 cityId" name="city">
                       <option value="">Select City</option>
                  </select>
                </div>
            </div> <!-- Row division Over -->


               <center><h3>OR</h3></center>

       <label for="sel1">Select Department:</label>
      <select class="form-control" id="sel1" name="department">
        <option unselected>Select</option>
        <option>CSE/IT</option>
        <option>ELECTRICAL</option>
        <option>ECE</option>
        <option>ICE</option>
        <option>CIVIL</option>
        <option>MECHANICAL</option>
        <option>CHEMICAL</option>
        <option>MEDICAL</option>
        <option>AGRICULTURE</option>
      </select>

         
      <input type="submit" name="submit" id="submit" value="Submit" class="btn btn-info pull-right">
    </div> <!-- Column division Off -->

  </form>
</div>
</div>

<!--<h2>Submitted variables</h2>-->
{{#= BEAUTIFY(request.vars)}}

{{#=form}}

{{#end}}












